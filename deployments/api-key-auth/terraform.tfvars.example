# terraform.tfvars - API Key Authentication Deployment
# Copy this to terraform.tfvars and fill in your actual values

# Confluent Cloud Authentication (required)
confluent_cloud_api_key    = "YOUR_CONFLUENT_CLOUD_API_KEY"
confluent_cloud_api_secret = "YOUR_CONFLUENT_CLOUD_API_SECRET"

# Existing Identity Provider (required) - choose one from your list  
existing_identity_provider_id = "op-4N"  # or "op-q0G"

# Environment (update with your environment ID)
environment_id = "env-nvv5xz"

# Azure Application Details (required)
azure_app_client_id = "YOUR_AZURE_APP_CLIENT_ID"